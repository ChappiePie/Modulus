{
  "schemaVersion": 1,
  "id": "modulus",
  "version": "${version}",
  "name": "Modulus",
  "description": "The core for the mods created by Chappie is under constant development. \nBasically adding a lot of cool things for convenient writing of mods.",
  "authors": [
    "ChappiePie"
  ],
  "contact": {
    "homepage": "https://discord.gg/DwTaMAr"
  },
  "license": "ARR",
  "icon": "assets/modulus/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "chappie.modulus.Modulus"
    ],
    "client": [
      "chappie.modulus.client.ModulusClient"
    ],
    "cardinal-components": [
      "chappie.modulus.ModulusComponents"
    ],
    "fabric-datagen": [
      "chappie.modulus.ModulusDataGenerator"
    ]
  },
  "custom": {
    "cardinal-components": [
      "modulus:powers",
      "modulus:player_anim"
    ]
  },
  "mixins": [
    "modulus.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.10",
    "minecraft": "~1.21.10",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "theboys": "*"
  }
}
